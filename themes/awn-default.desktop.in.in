[Desktop Entry]
Name=Awn Default Theme
Icon=avant-window-navigator
#Must be Theme
X-AWN-Type=Theme

#Orientation of Awn, top=0, right=1, bottom=2 and left=3
X-AWN-ThemeOrientation=2

#The user-visible size of Awn (minus the offset)
X-AWN-ThemeSize=48

#A bitmask which stores the type of effect for each event
X-AWN-ThemeEffects=69905

#The style of the bar, none=0, flat bar=1, 3d bar=2, curved bar=3
X-AWN-ThemeStyle=1

#First gradient step color.
X-AWN-ThemeGstep1=#efefebebe7e79b00

#Second gradient step color.
X-AWN-ThemeGstep2#efefebebe7e7c800

#First highlight gradient step color.
X-AWN-ThemeGhistep1=#ffffffffffffdc00

#Second highlight gradient step color.
X-AWN-ThemeGhistep2=#ffffffffffff2000

#Main border color.
X-AWN-ThemeBorder=#9a208c5e7e9cc800

#Internal border color.
X-AWN-ThemeHilight=#ffffffffffff6400

#Anything greater than 0 will draw a panel with corners. The greater the radius, the larger the corners.
X-AWN-ThemeCornerRadius=10

#The angle of the panel in 3D mode.
X-AWN-ThemePanelAngle=45

#The symmetry of the curve in Curves mode.
X-AWN-ThemeCurvesSymmetry=0.5

#The curviness of the panel in Curves mode.
X-AWN-ThemeCurviness=10
