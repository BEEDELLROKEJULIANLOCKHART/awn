<?xml version="1.0"?>
<node name="/org/awnproject/Awn/Panel">
  <interface name="org.awnproject.Awn.Panel">
    <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="awn_panel"/>
  
    <!-- Methods -->
    <method name="DeleteApplet">
      <arg name="uid" type="s" direction="in" />
    </method>

    <method name="SetAppletFlags">
      <arg name="uid" type="s" direction="in" />
      <arg name="flags" type="i" direction="in" />
    </method>

    <method name="InhibitAutohide">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg name="app_name" type="s" direction="in" />
      <arg name="reason" type="s" direction="in" />
      <arg name="cookie" type="u" direction="out" />
    </method>

    <method name="UninhibitAutohide">
      <arg name="cookie" type="u" direction="in" />
    </method>

    <method name="GetInhibitors">
      <arg name="inhibitors" type="as" direction="out" />
    </method>

    <method name="DockletRequest">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg name="min_size" type="i" direction="in" />
      <arg name="shrink" type="b" direction="in" />
      <arg name="window_id" type="x" direction="out" />
    </method>

    <!-- Signals -->
    <signal name="SizeChanged">
      <arg name="size" type="i" />
    </signal>

    <signal name="OrientChanged">
      <arg name="orient" type="i" />
    </signal>

    <signal name="OffsetChanged">
      <arg name="offset" type="i" />
    </signal>

    <signal name="PropertyChanged">
      <arg name="uid" type="s" />
      <arg name="prop-name" type="s" />
      <arg name="value" type="v" />
    </signal>

    <signal name="DestroyNotify"></signal>

    <signal name="DestroyApplet">
      <arg name="uid" type="s" />
    </signal>

    <!-- Properties -->
    <property name="Size" type="i" access="read" />
    <property name="MaxSize" type="i" access="read" />
    <property name="Orient" type="i" access="read" />
    <property name="Offset" type="i" access="read" />

  </interface>
</node>
