<?xml version="1.0"?>
<node name="/org/awnproject/Awn/Panel">
  <interface name="org.awnproject.Awn.Panel">
    <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="awn_panel"/>
  
    <!-- Methods -->
    <method name="DeleteApplet">
      <arg name="uid" type="s" direction="in" />
    </method>

    <method name="SetAppletFlags">
      <arg name="uid" type="s" direction="in" />
      <arg name="flags" type="i" direction="in" />
    </method>

    <method name="AppletSizeRequest">
      <arg name="uid" type="s" direction="in" />
      <arg name="width" type="i" direction="in" />
      <arg name="height" type="i" direction="in" />
    </method>

    <!-- Signals -->
    <signal name="SizeChanged">
      <arg type="i" name="size" />
    </signal>

    <signal name="OrientChanged">
      <arg type="i" name="orient" />
    </signal>

    <signal name="OffsetChanged">
      <arg type="i" name="offset" />
    </signal>

    <signal name="DestroyNotify"></signal>

    <signal name="DestroyApplet">
      <arg type="s" name="uid" />
    </signal>

  </interface>
</node>
