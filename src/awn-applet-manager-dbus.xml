<?xml version="1.0"?>
<node name="/org/awnproject/Awn/UA">
  <interface name="org.awnproject.Awn.UA">
    <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="awn_ua"/>
  
    <!-- Methods -->
    <method name="add_applet">
      <arg name="name" type="s" direction="in" />
      <arg name="uid" type="x" direction="in" />
      <arg name="width" type="i" direction="in" />
      <arg name="height" type="i" direction="in" />
      <arg name="size_type" type="s" direction="in" />
    </method>

    <method name="get_all_server_flags">
      <arg name="name" type="s" direction="in" />
      <arg name="hash" type="a{ss}" direction="out" />
    </method>

  </interface>
</node>
